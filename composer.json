{
    "name": "aiman/thaana-tinymce-field",
    "description": "A Laravel Nova Tinymce Field for Thaana",
    "keywords": [
        "laravel",
        "nova",
        "thaana",
        "tinymce"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Aiman\\ThaanaTinymceField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aiman\\ThaanaTinymceField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
